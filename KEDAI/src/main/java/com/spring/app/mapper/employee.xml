<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="employee">
	
	<!-- 가져오기 -->
	<select id="employee_list_select" resultType="MemberVO">
		select *
		from tbl_employees
	</select>
	
	<!-- 사원정보 가져오기 -->
	<select id="employee_get_select" parameterType="string" resultType="MemberVO">
		select *
		from tbl_employees
        where empid = #{empid}
	</select>
		
	
</mapper>